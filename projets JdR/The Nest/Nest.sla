                <ITEXT FONT="Courier New Bold" FONTSIZE="31.2" CH="L’univers"/>
                <para PARENT="Heading 1"/>
                <breakframe/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Histoire"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Développement technologique important sous le GM, avec la découverte d’un composé nouveau, l’anatase, qui sert à alimenter les nouvelles technologies."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Apparition progressives de «"/>
                <nbspace FONT="Liberation Serif Regular"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Dolines"/>
                <nbspace FONT="Liberation Serif Regular"/>
                <ITEXT FONT="Liberation Serif Regular" CH="», des trous sous la surface terrestre. Au premier abord juste des étrangetés, elles se sont multipliées, puis des rapports ont signalé qu’elles n’étaient pas vides…."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="C’est ainsi qu’est arrivée la première invasion, qui ravagea le monde et vit la chute du GM. "/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Avec la fin des institutions du monde d’avant, de nouvelles factions sont nées"/>
                <nbspace FONT="Liberation Serif Regular"/>
                <ITEXT FONT="Liberation Serif Regular" CH=": des survivants du GM fondèrent le GMR et GAIA par exemple. Ces nouvelles factions ont leurs problèmes"/>
                <nbspace FONT="Liberation Serif Regular"/>
                <ITEXT FONT="Liberation Serif Regular" CH=": leurs relations sont compliquées"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Factions"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
