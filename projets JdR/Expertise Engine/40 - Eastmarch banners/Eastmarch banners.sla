                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Priest"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialties: 1 religion, preaching or appeasing"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Masteries: scriptures 3, one weapon or craft 2"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Starting equipment: merchant’s clothes, scriptures, holy symbol."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Noble"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialties: politics, heraldry or strategy or small talks"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Masteries: domain management 3, dagger or sword or bow 2."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Starting equipment: noble clothes, dagger, domain."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Militia"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialties: spear or shield or crossbow, one settlement"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Masteries: bribes 2, shield or spear or crossbow 2, law 1"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Starting equipment: shield, spear or crossbow, gambeson, skull cap"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Mercenary"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialties: one weapon type, marching or thuggery"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Masteries: one weapon 3, one other weapon 2"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Starting equipment: gambeson, two weapons, kettle helmet."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Village’s healer"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialties: healing, negociations or occult secrets"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Masteries: one specific type of wounds 3, folk tales 2"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Starting equipment: bandages, herbs (3 bundles), peasant’s clothes."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Witch"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialties: rituals, occult secrets"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Masteries: one ritual 3, herbs and potions 2"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Starting equipment: peasant’s clothes, dagger"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Formative event"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Formative events are the reason the character went on the roads and seeks adventure or mercenary jobs. Each gives a specialty, and a related NPC. This NPC may be used during the campaign by the DM or may be suggested at some point by the player."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The players roll their formative event randomly. If they accept the result, they gain a character point. If not, they may choose the event instead of rolling."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Bastard"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The character is the illegitimate children of a ruler, local or important. It comes with great deal of disdain from the others, especially from legitimate children. Bastards often leave for adventure, generally to escape their family situation, sometimes because it is a good opportunity to rise the social ladder."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialty: one weapon type or intimidation or sneak."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="NPC: noble parent, step-parent or a legitimate sibling."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Broken heart"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The character has had a love affair that did not end well: their love interest died, or the love was not reciprocated. Anyway, the character decided to venture out to try and change their life, perhaps to come back to their loved one later."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialty: charm"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="NPC: love interest, or rival, or love interest parent."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Captured"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The character was enslaved or captured as a prisoner of war, which left them scarred in their mind if not in their body. They may not have anything to come back to, and choose to sell their skills."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialty: improvised weapons or sneak or survival"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="NPC: captor, companion of capture, liberator."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Escapee"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The character decided to run away from their previous life, hoping a wandering company will allow them to escape completely from their past. This was a choice made by the character."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialty: running or riding"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="NPC: parent or soon-to-be spouse or creditor."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Survivor"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The character was perhaps the only one to survive a raid, a battle or a natural catastrophe. Without anywhere to go, they now wander."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialty: sneaking"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="NPC: cause of the massacre, another survivor, someone that saved the character"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Treason"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The character was horribly betrayed by someone close: a friend tried to kill them, someone gave an important secret away, or their title was usurped by a rival family."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialty: politics"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="NPC: traitor or whoever ordered the treason to happen."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Wanderlust"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The character decided to wander the world, in search for its mysteries and wonders."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialty: marching or 1 culture"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="NPC: a journey companion, a mentor figure."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Character history"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT CH="The history of a character is his own personal narrative arc, that they may discover and fulfill during their adventure."/>
                <para PARENT="Text body" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Each character starts with the hook, and when the condition is fulfilled, they may unlock the results. The exact steps between the condition and the results, and what exactly are the results are left to the GM."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The player may roll randomly their history, and gain a character point, or if they don’t like the result, they may choose their history."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
