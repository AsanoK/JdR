                <ITEXT FONT="Liberation Serif Regular" CH="Masteries: "/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Starting equipment: peasant’s clothing, farm tools."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Hunter"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialties: hunting, survival or ranged weapons."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
