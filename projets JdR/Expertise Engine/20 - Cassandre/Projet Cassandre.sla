        <PAGEOBJECT XPOS="140.338983050848" YPOS="15052.7532363539" OwnPage="17" ItemID="177218544" PTYPE="4" WIDTH="250.169491525424" HEIGHT="760.271186440678" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L250.169 0 L250.169 760.271 L0 760.271 L0 0 Z" copath="M0 0 L250.169 0 L250.169 760.271 L0 760.271 L0 0 Z" gXpos="140.338983050847" gYpos="1824.40677966102" gWidth="0" gHeight="0" PSTYLE="Default Paragraph Style" LAYER="0" NEXTITEM="177204384" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Default Paragraph Style" LINESPMode="1" LINESP="12"/>
                <ITEXT CH="Character creation"/>
                <para PARENT="Heading 1" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="11.9905511811024" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT CH="Step 1: Culture"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="First step to create a character is to select what type of culture they have, which is mostly determined by their familial environment (or lack thereof)."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Sans Bold" FONTSIZE="14.14" CH="Brinker"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="+1 Engineering, -1 Physical, a specialty related to the settlement where the character comes from."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Sans Bold" FONTSIZE="14.14" CH="Corporate upbringing"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="+1 Systems or Social, -1 Combat or Physical. A specialty related to the corporation or a state the character belongs to."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Sans Bold" FONTSIZE="14.14" CH="Helix clone"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="+1 Physical or Engineering, -1 Social. The Clone specialty."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Negative trait: Clone"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Positive traits: peer (clones)"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Living in slums"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="+1 Combat or social, -1 Systems. A specialty related to the specific place the character is from."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Step 2: Education"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT CH="Technical education"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Technical education is mostly the standard education outside corporate life. It mostly consists in learning a small family of jobs and knowledge required to do them."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="+1 attribute related to the specialty, -1 other attribute"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="One technical specialty, corresponding to the job the character was trained to do. Professional education is common for many maintenance teams, military, police forces, some medical professions."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Examples: Security guard, life system maintenance, airspace maintenance, pilot, firemen, emergency teams, printing technician, warehouse management, network maintenance …."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Academic education"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Academic education is broader and longer than technical one. It tends to approach knowledge as a whole, to pick a specialty after education. This is the corporate standard, but can also be the case in law and health domains for instance."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="+1 Social or Investigation, -1 Combat or Physical"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="One academic specialty, corresponding to the domain the character has studied: law, airspace engines, astrophysics, risk management, finance, politics…."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Street smart"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The character does not have any certified education, and learned its trade through someone, or just by looking at what’s done. Most of the time it requires no qualifications. It is the norm in most of earth’s slums, but can also be the case in the poorer parts of many stations."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="A specialty from Street Smart education is what the character would have done for a living during their teens and early in their life, legal."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="+1 Social or Investigation, -1 Systems or Engineering"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Some options: retail, handling and delivery, barista, handiwork, workshop job "/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Sans Bold" FONTSIZE="14.14" CH="Criminal life"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The character’s trade was illegal in most jurisdiction. Most don’t do it by choice, but out of necessity, and it is pretty widespread."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="A specialty from criminal life corresponds to what the character does the most: con jobs, assault, burglary, smuggling, counterfeit, prison life, drug dealing…."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="+1 Social or Combat or Physical, -1 Systems or Engineering."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Step 3: Profession"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The profession is what the character usually does to bring money in. That may not be their current profession, but the one that defines them the most."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Each profession brings the character 2 specialties related to their job, as well as 5 mastery points."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Police officer"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Police officer may belong to any organization tasked with maintaining public order and/or investigating crimes under a more or less definite jurisdiction: small settlement’s militia, corporate internal affairs team or Sol System police force."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Example of specialties: law, crime scene management, ballistic analysis, SWAT techniques, DNA testing, interrogation, search, arrest techniques, linking clues."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Basic equipment: light armor, pistol, zip ties, clue management system or DNA testing kit or police vehicle."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Security agent"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Security agents is often a euphemism for either corporate combat teams, or mercenaries, working either freelance or part of some company providing violence as a service (VaaS). They are most of the time despised for their violent profession, and many take in former soldiers regardless of former allegiances."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Example of specialties: security procedures, emergency procedures, gunfight, dynamic entry, combat team coordination."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Basic equipment: rifle or pistol, combat armor, scouting drone or targeting pod."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Technical expert/consultant"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Experts have a very specific set of knowledge, that is often really searched after by corporations or local administrations, and they provide more-or-less temporary support to existing teams or intervene to bring whole solutions."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Example of specialties: system security, marketing techniques, [technical] procedure, law, job market."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Basic equipment: technical tools, "/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Journalist"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Journalists are searching for the next bit of information that will be a big hit: interesting story about someone, sex scandal, analysis of current regional events…. Freelance and corporate coexist, even though they don’t often like each other."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Example of specialties: interrogation, media creation, social network, I know someone that can help, linking clues, negotiations."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Basic equipment: quality video equipment, marketing analysis tools or clue management system"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Criminal"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="It’s not honest, but it’s work! The social implications depend a lot on what type of crime is committed, where and against who: smugglers may be seen as integral parts of some communities, as may be soldiers working for organized crime. Thieves and con men are mostly despised."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Example of specialties: not being seen, brawl, gunfight, con, system breaching, breaking in, intimidation, piloting, hiding things, criminal network."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Basic equipment: pistol, smuggler’s ship or stash of drugs or rifle."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Step 4: Hobby"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Each character has a hobby, that brings them one specialty."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Astronomy"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="A popular hobby for those living in space stations or on exoplanets. Still a dream for many as well on earth."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialty: local solar system or astrophysics or Gates."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Clubbing"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Going out and meeting people is still one of the most common hobbies you can have, nearly all stations have somewhere to hang out, more or less legal."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialty: clubs of a specific settlement or socialites of a specific settlement."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Social networks"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="You can get to know quite a lot of people and things on social networks, but that’s not easy : either you’re good at navigating such networks, or maybe you’re producing content to share."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialty: social network marketing or media creation."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Sports"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Physical activities are still an all-time favorite, either to practice or to follow. Space stations have at least gym equipment, and you can find whole brands of 0-G sports that start to be popular. On planetary settlements, it depends a lot on gravity to determine what may be practiced."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="And a lot of people still follow Earth’s games and teams. Olympic games have seen the first non-earth states participate recently."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialty: A specific sport or sports fans"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Video games"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Video games are still a very popular support, available pretty much everywhere to everyone. Huge networked games are somewhat rare, due to lag constraints, but some managing games are still huge. Solo and small cooperative games are wildly popular across the entirety of human settlements."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Specialty: video game knowledge, video gamer network"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Step 5: Finishing touches"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The player may finally allocate 10 mastery points, on a maximum of 5 different masteries."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Sans Bold" FONTSIZE="18.2" CH="Elements of a character"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="11.9905511811024" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT CH="Attributes"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT CH="Engineering"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Engineering will be used for most technology that does not rely on networks, but may use calculations or memory: understanding an engine, biology or astrophysics."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="For characters, it may be useful to understand why a piece of equipment is broken, how to fix it, or analyze some clues."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Investigation"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="This attribute will be used to search for something, or someone, link clues together, or searching through a variety of data to get one specific piece of information."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="It represents the ability of the character to find something hidden, to make a logical deduction or to interrogate a suspect."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Physical "/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The physical prowess of the character, that may come into play whenever they need sport abilities, hand-to-hand combat or just reaction speed. It encompasses both might and endurance."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="A pilot may use it to react to something coming fast, a fighter for close quarter combat, and an investigator to gain access to somewhere difficult, or run after a suspect. "/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Precision"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The ability of the character to control what they are doing with their body. It may be used to shoot something far away, run a ship in a tight spot, or dance in a graceful manner."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Social"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="This attribute governs most interactions the character may have with others, and to understand how interactions work between others. "/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="One could use it to bargain for a better price, intimidate a suspect or seduce someone at the bar."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Systems"/>
                <para PARENT="Heading 3" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="The attribute for anything network-related, from AIs to basic communication networks. Basically the IT attribute."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="For example: to enter into a secured network, understand an AI system or jam communications."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Character traits"/>
                <para PARENT="Heading 1" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="11.9905511811024" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT CH="Positive character traits"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT CH="Negative character traits"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT CH="Gear and technology"/>
                <para PARENT="Heading 1" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="11.9905511811024" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT CH="Rules"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Tag: Networked. It denotes an item that requires access to a network to work properly, and thus can be hacked through this very network."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="General tech and gear"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT CH="Network gear"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Electronic warfare equipment: may be used to jam communication and identify network relays in close range."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Network defense systems"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Software incursion suite"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Investigation and espionage gear"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Data analysis system: A software made to collect and analyze huge amount of data from multiple points of entry: video, sound, numbers, tables or plain text. It may be used to search for a specific data point, or to get patterns in the dataset. Tags: tool (2-5, depending on quality), networked."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Database uplink: an entry point into a specific database, that may be used to run searches in it. Security services may have access to a registry of known offenders, media to a “who’s who” list for example. Tags: tool (2-5, depending on quality), networked."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Entry tools: a set of tools made to open locked doors. Lock-picks are out of fashion, but maglocks and card readers can still be fooled by a good toolset. Tags: tool (2-5, depending on quality), networked."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Investigation assistant: an AI-powered assistant, made to display clues in optimized ways, as well as handling certain part of the paperwork and storage policy. Tags: tool (2), networked."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Multimedia toolkit: a set of cameras, light and microphones, used to create video, photographic or sound content easily and quickly. The setup is mostly automatic. Tags: tools (2-5, depending on quality), networked."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Zip-ties: small but heavy-duty plastic sip-ties, to restrain a suspect for up to a few hours. Some criminal groups use them as well to kidnap their targets. A character in zip-locks must succeed a standard check versus a difficulty 4."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Weapons and protection"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Combat armor: A military-grade protection, meant to engage armed opponents, especially in close quarter. Tags:protection (9)"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Flak vest: a basic protection, meant to help security groups to withstand small caliber fire. Tags: protection (4)."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Incapacitating grenade: generating noise, light and shock, this grenade is part of the “less-than-lethal” options for security groups. It is very popular in space stations in its non shock-wave version, because it does not present risk to the infrastructure. Tags: Stressful 3 (4 in the shock-wave version), blunt, non-lethal, ranged(0), multiple targets (3)"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Pistol: a typical sidearms carried by almost all security forces, existing in a variety of versions and manufacturers, some are less-than-lethal options. Tags (normal version): ranged(1). Tags (non-lethal): ranged(1), blunt, non-lethal."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Targeting autopod: a small system analyzing gunshot patterns and tactical networks to identify and lock enemies, which may then be targeted more efficiently. It works very well in conjunction with smartgun systems. Effect: at the end of each turn, the character may gain a targeting token, as if doing the targeting action. Tags: ranged (2), networked."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Taser: small sidearm to control suspects. Depending on local jurisdiction, most people are allowed to carry one without questions by the police, when a gun might raise suspicions. Tags: ranged (0), non-lethal."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="Shrapnel rifle: a short rifle, made for close quarter engagement. It shoots a volley of small fragments, made to shred “soft” targets, but leave the infrastructure pretty intact. Tags: ranged (1), deadly(1), blunt."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="SMG: a small caliber high capacity gun, made to spray bullets very quickly, and be small enough to work in close quarter combat. It may be seen with suspicion by police. Tags: ranged (1), stressful (2), multiple targets (2)"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Progress of medicine"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
            </StoryText>
        </PAGEOBJECT>
