                <para PARENT="Text body" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Critical strike"/>
                <para PARENT="Heading 4" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="6.00944881889764" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="IC: 6"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Effects: the character may attack an identified weak point of an enemy. If the strike succeeds, the attack gains the tags lethal and piercing."/>
                <para PARENT="Text body" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Tags: attack, weapon-range, charging"/>
                <para PARENT="Text body" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Reconstruct"/>
                <para PARENT="Heading 4" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="6.00944881889764" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="IC: 6"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Effects: the character may come back, even from horrible wounds. The character immediately regains [Stamina] HP and bring themselves back on their feet."/>
                <para PARENT="Text body" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Tags: heal, self"/>
                <para PARENT="Text body" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Raise the banner high"/>
                <para PARENT="Heading 4" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="6.00944881889764" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="IC: 2"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Effects : The character may raise up a symbol of their authority or pride, to bring more courage to their allies. Targeted characters will reduce the IC of their next action by 1."/>
                <para PARENT="Text body" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Tags: Targets (5), buff, short range."/>
                <para PARENT="Text body" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT CH="Being wounded, dying and the spark"/>
                <para PARENT="Heading 2" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Sans Bold" FONTSIZE="14.14" CH="Wounds"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="A character may suffer a number of wounds equal to their HP before being out of action."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Sans Bold" FONTSIZE="14.14" CH="Death"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="7.00157480314961" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="An out-of-action character may be killed by a creature during a fight, by using the Kill special action. If every character is the group is put out of action, the whole group is considered killed."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="However, an Ouroboros character is special because of their Spark, and will be brought back by whatever force is behind this Spark: they “respawn” back at a nearby Haven (most probably the last they visited)."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <ITEXT FONT="Liberation Serif Regular" CH="But dying has its cost, anytime the group dies completely, there is some narrative consequence: a haven is destroyed, the creature gets stronger for the next face-off, a friendly NPC gets killed"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="7.00157480314961" BCOLOR="None"/>
                <breakframe/>
                <ITEXT CH="Equipments"/>
                <para PARENT="Heading 1" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="11.9905511811024" NACH="6.00944881889764" BCOLOR="None"/>
